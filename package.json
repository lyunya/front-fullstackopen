    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "eslint-plugin-jest": "^24.1.3",
    "json-server": "^0.15.1"
  },
  "proxy": "http://localhost:3001"
